CREATE TABLE T_HL_YW_TNBFYQK (
ID VARCHAR(32) NOT NULL
, BLWSID VARCHAR(32) NOT NULL
, YWMC VARCHAR(200) NOT NULL
, FYCS NUMBER(2) NOT NULL
, FYSJ_ZA VARCHAR(2) NOT NULL
, FYSJ_ZO VARCHAR(2) NOT NULL
, FYSJ_W VARCHAR(2) NOT NULL
, FYSJ_S VARCHAR(2) NOT NULL
, FYL NUMBER(4,1) NOT NULL
);
ALTER TABLE T_HL_YW_TNBFYQK ADD CONSTRAINT  T_HL_YW_TNBFYQK_PK PRIMARY KEY (id);
CREATE CLUSTER INDEX T_HL_YW_TNBFYQK_CLS ON T_HL_YW_TNBFYQK(blwsid);
COMMENT ON COLUMN T_HL_YW_TNBFYQK.ID IS '主键ID';
COMMENT ON COLUMN T_HL_YW_TNBFYQK.BLWSID IS '病人文书索引唯一键';
COMMENT ON COLUMN T_HL_YW_TNBFYQK.YWMC IS '药物名称';
COMMENT ON COLUMN T_HL_YW_TNBFYQK.FYCS IS '每日用量（次/日）';
COMMENT ON COLUMN T_HL_YW_TNBFYQK.FYSJ_ZA IS '服药时间（早）';
COMMENT ON COLUMN T_HL_YW_TNBFYQK.FYSJ_ZO IS '服药时间（中）';
COMMENT ON COLUMN T_HL_YW_TNBFYQK.FYSJ_W IS '服药时间（晚）';
COMMENT ON COLUMN T_HL_YW_TNBFYQK.FYSJ_S IS '服药时间（睡前）';
COMMENT ON COLUMN T_HL_YW_TNBFYQK.FYL IS '每次服药（G）';
COMMENT ON TABLE T_HL_YW_TNBFYQK IS '糖尿病服药情况';
