CREATE TABLE T_HL_DIC_HLXM (
ID VARCHAR(32) NOT NULL
, YLJGID VARCHAR(32)
, XMMC VARCHAR(200) NOT NULL
, XMDM VARCHAR(32)
, JE NUMBER(10,2)
, DEL_FLAG VARCHAR(2) DEFAULT '0' NOT NULL
, CREATE_BY VARCHAR(32)
, CREATE_TIME DATETIME(0) DEFAULT SYSDATE NOT NULL
, UPDATE_BY VARCHAR(32)
, UPDATE_TIME DATETIME(0) DEFAULT SYSDATE NOT NULL
, REMARKS VARCHAR(255)
) STORAGE(FILLFACTOR 95, ON TS_HLYTH_KEEP);
ALTER TABLE T_HL_DIC_HLXM ADD CONSTRAINT  T_HL_DIC_HLXM_PK PRIMARY KEY (id);
CREATE INDEX T_HL_DIC_HLXM_IDX1 ON T_HL_DIC_HLXM(yljgid);
COMMENT ON COLUMN T_HL_DIC_HLXM.ID IS '主键ID';
COMMENT ON COLUMN T_HL_DIC_HLXM.YLJGID IS '医疗机构ID';
COMMENT ON COLUMN T_HL_DIC_HLXM.XMMC IS '项目名称';
COMMENT ON COLUMN T_HL_DIC_HLXM.XMDM IS '项目代码';
COMMENT ON COLUMN T_HL_DIC_HLXM.JE IS '金额';
COMMENT ON COLUMN T_HL_DIC_HLXM.DEL_FLAG IS '删除标志';
COMMENT ON COLUMN T_HL_DIC_HLXM.CREATE_BY IS '创建人';
COMMENT ON COLUMN T_HL_DIC_HLXM.CREATE_TIME IS '创建时间';
COMMENT ON COLUMN T_HL_DIC_HLXM.UPDATE_BY IS '更新人';
COMMENT ON COLUMN T_HL_DIC_HLXM.UPDATE_TIME IS '更新时间';
COMMENT ON COLUMN T_HL_DIC_HLXM.REMARKS IS '备注';
COMMENT ON TABLE T_HL_DIC_HLXM IS '护理项目字典表';
